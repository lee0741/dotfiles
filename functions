function mkcd() {
  mkdir $1
  cd $1
}

function cl() {
  cc $1 | less -R
}

function jp () {
  cat $1 | python -m json.tool
}

function youtube () {
  export http_proxy=http://127.0.0.1:8087
  youtube-dl -w -l -f 18 $1
}

function magnet () {
  python3 ~/projects/torrent2magnet/torrent2magnet.py $1 | tr -d "\n" | pbcopy
}

function mdy () {
  http POST http://fuckyeahmarkdown.com/go/ u==$1 > tmp.md  
}

function regex () {
   python -c "import re; re.compile(r'$1',re.DEBUG)"  
}
